{
    "name": "model",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/model/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/model/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "build": {
            "executor": "@nx/rollup:rollup",
            "outputs": ["{options.outputPath}"],
            "options": {
                "tsConfig": "libs/model/tsconfig.lib.json",
                "project": "libs/model/package.json",
                "entryFile": "libs/model/src/index.ts",
                "external": ["react", "react-dom", "react/jsx-runtime"],
                "rollupConfig": "@nx/react/plugins/bundle-rollup",
                "compiler": "tsc",
                "format": ["esm", "cjs"],
                "buildableProjectDepsInPackageJsonType": "dependencies",
                "updateBuildableProjectDepsInPackageJson": true,
                "generateExportsField": true,
                "assets": [
                    {
                        "glob": "libs/model/README.md",
                        "input": ".",
                        "output": "."
                    },
                    {
                        "glob": "libs/model/CHANGELOG.md",
                        "input": ".",
                        "output": "."
                    }
                ],
                "outputPath": "dist/libs/model"
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/model/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "deploy": {
            "executor": "ngx-deploy-npm:deploy",
            "options": {
                "access": "public"
            }
        }
    }
}
